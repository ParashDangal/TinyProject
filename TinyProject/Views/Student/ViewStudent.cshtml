@model List<TinyProject.Models.Domain.StudentModel>

<div class="container">
    <div class="p-3">
        <h5>List of Students</h5>
         <div class="my-2">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">Id</th>
                        <th scope="col">Name</th>
                        <th scope="col">Address</th>
                        <th scope="col">Email</th>
                        <th scope="col">Phone</th>
                        <th scope="col">Action</th>
                    </tr>
                </thead>
                @if(Model!=null){
                <tbody>
                @foreach (var data in Model)
                {
                    <tr>
                        <th scope="row">@data.StudentId</th>
                        <td>@data.Name</td>
                        <td>@data.Address</td>
                        <td>@data.Email</td>
                        <td>@data.Phone</td>
                        <td>
                            <a href="#">Edit</a>
                            <a href="#">Delete</a>
                        </td>
                    </tr>
                }
                    </tbody>
                }
                else
                {
                    <div class="alert alert-danger" role="alert">
                       No Data Found !!!
                    </div>
                }
            </table>
         </div>
    </div>
</div>